{
  "name": "pdcidadaodoc",
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/(.+)",
      "dest": "public/$1"
    }
  ]
}
